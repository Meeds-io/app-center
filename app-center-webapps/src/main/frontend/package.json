{
  "name": "app-center",
  "version": "1.0.0",
  "description": "app center portlets",
  "scripts": {
    "//": "Option --progress: print compilation progress in percentage",
    "start": "webpack-dashboard webpack-dev-server -d --hot --config webpack.dev.js --watch",
    "dev": "webpack --mode development --config webpack.dev.js",
    "build": "webpack -p --progress --trace-deprecation --config webpack.prod.js",
    "watch": "webpack --config webpack.watch.js --progress --colors --watch -d"
  },
  "author": "eXo",
  "license": "LGPL",
  "repository": {
    "type": "git",
    "url": "https://github.com/exo-addons/app-center"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@fortawesome/vue-fontawesome": "^0.1.5",
    "axios": "^0.18.0",
    "bootstrap-vue": "^2.0.0-rc.11",
    "i": "^0.3.6",
    "jquery": "^3.3.1",
    "moment": "^2.23.0",
    "npm": "^6.7.0",
    "terser": "3.14.1",
    "vue": "^2.5.17",
    "vue-avatar": "^2.1.7",
    "vue-bootstrap-datetimepicker": "^5.0.1",
    "vue-d2b": "^1.0.14",
    "vue-moment": "^4.0.0",
    "vue-text-dot": "^1.0.0",
    "vuejs-datepicker": "^1.5.4",
    "vue-esc": "^3.0.1",
    "vue-loading-overlay": "3.2.0",
    "v-tooltip": "2.0.2"
  }
}
